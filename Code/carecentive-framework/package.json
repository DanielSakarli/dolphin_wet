{
	"name": "carecentive-framework",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"load-seed-data": "npx knex seed:run --knexfile ./knexfile.js",
		"dev": "nodemon app.js",
		"insmysql": "npm install mysql",
		"core-migrate-all": "./node_modules/carecentive-core/npx knex migrate:latest",
		"migrate-all": "npx knex migrate:latest",
		"rollback-all": "npx knex migrate:rollback --all",
		"poll-data": "node scripts/pollData.js",
		"test-push": "node scripts/WebPush.js",
		"test": "jest --watch --runInBand --detectOpenHandles"
	},
	"dependencies": {
		"@carecentive/carecentive-core": "^0.9.1",
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "~1.4.4",
		"crypto-js": "^4.1.1",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"ejs": "~2.6.1",
		"express": "~4.16.1",
		"express-validator": "^7.0.1",
		"form-data": "^3.0.0",
		"http-errors": "~1.6.3",
		"http-status-codes": "^2.2.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.21.13",
		"moment": "^2.29.4",
		"morgan": "~1.9.1",
		"multiparty": "^4.2.2",
		"mv": "^2.1.1",
		"mysql2": "^2.3.3",
		"node-cron": "^3.0.0",
		"nodemon": "^2.0.22",
		"objection": "^2.2.3",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/jest": "^29.5.2",
		"jest": "^29.5.0",
		"supertest": "^6.3.3"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/__tests__/setup.js"
		]
	}
}
